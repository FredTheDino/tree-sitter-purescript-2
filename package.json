{
  "name": "tree-sitter-purescript",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for PureScript",
  "main": "grammar.js",
  "author": "",
  "license": "MIT",
  "gypfile": true,
  "scripts": {
    "watch": "watchexec -N -r -e js -- tree-sitter generate &",
    "watch-test": "watchexec -N -r -e js -- 'tree-sitter generate && tree-sitter test' &"
  },
  "tree-sitter": [
    {
      "scope": "source.purescript",
      "file-types": [
        "purs"
      ],
      "injection-regex": "purescript"
    }
  ],
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  }
}

